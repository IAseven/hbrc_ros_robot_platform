#<---------------------------------------- 100 Characters ---------------------------------------->#

function scad() {
    DQ='"' # DQ == Double Quote
    FOO="name=$DQ$1$DQ" # $1 == the value to set name to
    openscad "$HR2_DIRECTORY/mechanical/hr2_models.scad" -D $FOO
}
case "$1" in
    "encoder_board")
	scad "encoder_board"
	;;
    "expansion_flat")
	scad "expansion_flat"
	;;
    "expansion_plate")
	scad "expansion_plate"
	;;
    "f1x4lp")
	scad "f1x4lp"
	;;
    "hr2_arm_assembly")
	scad "hr2_arm_assembly"
	;;
    "hr2_base_assembly")
	scad "hr2_base_assembly"
	;;
    "hr2_master_assembly")
	scad "hr2_master_assembly"
	;;
    "hr2_nucleo_assembly")
	scad "hr2_nucleo_assembly"
	;;
    "hr2_pi_assembly")
	scad "hr2_pi_assembly"
	;;
    "hr2_wheel_assembly")
	scad "hr2_wheel_assembly"
	;;
    "master_board")
	scad "master_board"
	;;
    "master_center_with_nucleo_board")
	scad "master_center_with_nucleo_board"
	;;
    "master_center_without_nucleo_board")
	scad "master_center_without_nucleo_board"
	;;
    "master_ne_board")
	scad "master_ne_board"
	;;
    "master_nw_board")
	scad "master_nw_board"
	;;
    "master_se_board")
	scad "master_se_board"
	;;
    "master_sw_board")
	scad "master_sw_board"
	;;
    "master_union_board")
	scad "master_union_board"
	;;
    "master_without_nucleo_board")
	scad "master_without_nucleo_board"
	;;
    "nucleo_144_board")
	scad "nucleo_144_board"
	;;
    "raspi_f2x20_board")
	scad "raspi_f2x20_board"
	;;
    "romi_base")
	scad "romi_base"
	;;
    "rpi4_ethernet_heat_sink")
	scad "rpi4_ethernet_heat_sink"
	;;
    "rpi4_processor_heat_sink")
	scad "rpi4_processor_heat_sink"
	;;
    "rpi4_sdram_heat_sink")
	scad "rpi4_sdram_heat_sink"
	;;
    "rpi4_usb_heat_sink")
	scad "rpi4_usb_heat_sink"
	;;
    "sbc_other_pi_board")
	scad "sbc_other_pi_board"
	;;
    "sbc_raspi4_board")
	scad "sbc_raspi4_board"
	;;
    "sonar_centered_board")
	scad "sonar_centered_board"
	;;
    "sonar_vertical")
	scad "sonar_vertical"
	;;
    "st_adapter_board")
	scad "st_adapter_board"
	;;
    "st_link_board")
	scad "st_link_board"
	;;
    "st_mate_board")
	scad "st_mate_board"
	;;
    "wheel_assembly")
	scad "wheel_assembly"
	;;
    *)
	echo "encoder_board"
	echo "expansion_flat"
	echo "expansion_plate"
	echo "f1x4lp"
	echo "hr2_arm_assembly"
	echo "hr2_base_assembly"
	echo "hr2_master_assembly"
	echo "hr2_nucleo_assembly"
	echo "hr2_pi_assembly"
	echo "hr2_wheel_assembly"
	echo "master_board"
	echo "master_center_with_nucleo_board"
	echo "master_center_without_nucleo_board"
	echo "master_ne_board"
	echo "master_nw_board"
	echo "master_se_board"
	echo "master_sw_board"
	echo "master_union_board"
	echo "master_without_nucleo_board"
	echo "nucleo_144_board"
	echo "raspi_f2x20_board"
	echo "romi_base"
	echo "rpi4_ethernet_heat_sink"
	echo "rpi4_processor_heat_sink"
	echo "rpi4_sdram_heat_sink"
	echo "rpi4_usb_heat_sink"
	echo "sbc_other_pi_board"
	echo "sbc_raspi4_board"
	echo "sonar_centered_board"
	echo "sonar_vertical"
	echo "st_adapter_board"
	echo "st_link_board"
	echo "st_mate_board"
	echo "wheel_assembly"
	;;
esac
